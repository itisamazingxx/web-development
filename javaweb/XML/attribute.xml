<?xml version="1.0" encoding="utf-8" ?>
<!--
    属性值用双引号(")或单引号(')分隔 (如果属性值中有'，用"分隔; 有"，用'分隔)
    一个元素可以有多个属性 它的基本格式为:<元素名 属性名="属性值">
    特定的属性名称在同一个元素标记中只能出现一次
    属性值不能包括& 字符
-->
<students>
    <student id = "100">
        <name>cru</name>
        <age>22</age>
    </student>
    <student id = "200">
        <name>john</name>
        <age>32</age>
    </student>
</students>